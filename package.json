{
  "name": "my-vscode-ish",
  "version": "0.1.0",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.json",
    "build": "npm run build:renderer && npm run build:main && electron-builder --win",
    "start": "electron ."
  },
  "devDependencies": {
    "electron": "^26.0.0",
    "electron-builder": "^24.0.0",
    "vite": "^5.0.0",
    "typescript": "^5.0.0",
    "concurrently": "^8.0.0",
    "wait-on": "^7.0.0"
  },
  "dependencies": {
    "monaco-editor": "^0.36.1"
  }
}
